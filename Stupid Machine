#include <iostream>
using namespace std;

int main() {
long long int t,i,j,x,y;
cin>>t;
while(t--){y=0;long long int n;
    cin>>n;
   long long int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
 x=a[0];
 while(x--){y+=n;
     for(i=0;i<n;i++){
         a[i]--;
         if(a[i]<=0){
             n=i;
         }
     }
 }
 cout<<y<<"\n";
}
	return 0;
}
