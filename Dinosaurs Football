#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,n,k,i;
	cin>>t;
	while(t--){
	    cin>>n>>k;
	    int a[n],j=0;
	    for(i=0;i<n;i++){
	        a[i]=i+1;
	        
	    }
	    j=a[k];
	    a[k]=a[n-1];
	    a[n-1]=j;
	     for(i=0;i<n;i++){
	   cout<<a[i]<<" ";
	    }
	    	cout<<"\n";
	}

	return 0;
}
