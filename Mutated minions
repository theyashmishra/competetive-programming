#include <iostream>
using namespace std;

int main() {
	long long int i,n,k,t,j;
	cin>>t;
	while(t--){j=0;
	    cin>>n>>k;
	    long int a[n];
	    for(i=0;i<n;i++){
	        cin>>a[i];
	        a[i]=a[i]+k;
	        if(a[i]%7==0){
	            j++;
	        }
	    }
	    cout<<j<<"\n";
	}
	return 0;
}
