#include <iostream>
using namespace std;

int main() {
long long int t,i,j,n;
cin>>t;
while(t--){n=j=0;
    char x[10],y[10];
    cin>>x>>y;
    for(i=0;x[i]!='\0';i++){
        if((x[i]!=y[i])&&(x[i]!='?'&&y[i]!='?')){
          
            n=1;
            break;
        }
    }
    if(n==1){
        cout<<"No\n";
    }
    else{
        cout<<"Yes\n";
    }
}
	return 0;
}
