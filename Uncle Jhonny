#include <iostream>
using namespace std;

int main() {
int t,n,i,j,x;
cin>>t;
while(t--){int y=0;
    cin>>n;
   long long int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
    cin>>x;
    for(i=0;i<n;i++){
        if(a[i]<a[x-1]&&i!=(x-1)){
            y++;
        }
    }
    cout<<y+1<<"\n";
    
}
	return 0;
}
