#include <bits/stdc++.h>

using namespace std;

int main() {
	long long int t,n,i,j,q,k,x,y,z;
	cin>>t;
	while(t--){
	    cin>>n>>q;
	    long long int a[n];
	    for(i=0;i<n;i++){
	        cin>>a[i];
	    }
	    sort(a,a+n);
	    while(q--){x=y=0;j=0;
	        cin>>k;
	    for(i=n-1;i>=0;i--){
	        if(k<=a[i]){
	            x++;
	        }
	        else if(k>a[i]){y=a[i];
	            while(y<k&&i!=j){
	                y+=1;
	                j++;
	            }
	            if(y==k){x++;}
	            if(i==j){
	                break;
	            }
	            
	        }
	}
	cout<<x<<"\n";
	}
	}
	return 0;
}
