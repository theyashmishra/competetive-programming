#include <iostream>
using namespace std;

int main() {
	int t,n,i,x;
	cin>>t;
	while(t--){
	    cin>>n;
	    long int a[n];
	    for(i=0;i<n;i++){
	        cin>>a[i];
	    }
	    x=a[0];
	    for(i=1;i<n;i++){
	        if(x>a[i]){
	    x=a[i];
	            
	        }
	}
	 cout<<x<<"\n";   
	}
	
	return 0;
}
