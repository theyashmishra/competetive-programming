#include <iostream>
using namespace std;

int main() {
	long int t,n,a,i,j,b;
	;
	cin>>t;
	while(t--){
	    cin>>n;
	    a=1;j=0;
	    for(i=0;i<n;i++){
	       if(i==0){
	           cout<<a<<" ";
	           a+=1;
	       }
	        else if(i==1){
	           cout<<a<<" ";
	           a+=2;
	       }
	         else{
	           cout<<a<<" ";
	           a+=3;
	       }
	    }
	    cout<<"\n";
	       
	}
	return 0;
}
