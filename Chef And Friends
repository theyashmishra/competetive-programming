#include <bits/stdc++.h>
#include<string.h>
using namespace std;

int main() {
	long int t=0,n,i,j=0,x;
	    cin>>n;
	    char str[20];
	    while(n--){
	        cin>>str;
	        for(i=0;str[i]!='\0';i++){
	            if((str[i]=='c'&&str[i+1]=='h')||(str[i]=='h'&&str[i+1]=='e')||(str[i]=='e'&&str[i+1]=='f')){
	               j++;
	               break;
	            }
	        }
	    }
	    cout<<j<<endl;
	
	return 0;
}
