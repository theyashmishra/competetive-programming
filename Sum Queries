#include <iostream>
using namespace std;

int main() {
	long long int t,i,x,y,z,k;

	    cin>>x>>y;
	    while(y--){
	        k=0;
	        cin>>i;
	        if((i>=0&&i<=x)||(i>(3*x))){
	            k=0;
	        }
	        else if(i>x&&i<=(2*x)){
	            k=i-(x+1);
	           
	        }
	        else if(i>(2*x)&&i<=(3*x)){
	            k=(3*x)-i;
	            k+=1;
	        }
	        cout<<k<<endl;
	    
	}
	return 0;
}
