#include <bits/stdc++.h>
using namespace std;

int main() {
	long long int t,n,i,j,x;
	cin>>t;
	while(t--){x=0;
	    cin>>n;
	    for(i=1;i<=sqrt(n);i++){
	        if(n%i==0){
	            if(n/i==i){
	                x+=i;
	            }
	            else{
	                x+=i;
	                x+=(n/i);
	            }
	        }
	    }
	    cout<<x<<endl;
	}
	return 0;
}
