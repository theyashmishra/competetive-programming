#include <iostream>
using namespace std;

int main() {
long long int i,j,t,n,x;
cin>>t;
while(t--){x=0;
    cin>>n;
    long long int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
    for(i=0;i<n-2;i++){
        if(a[i]==a[i+1]&&a[i+1]==a[i+2]){
            x=1;
            break;
        }
    }
    (x==1)?cout<<"Yes\n":cout<<"No\n";
}
	return 0;
}
