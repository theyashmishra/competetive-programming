#include <bits/stdc++.h>
using namespace std;
int main(void) {
long long int i,j,k,t,n;
float x,y,z;
cin>>t;
while(t--){y=0;z=0;
    cin>>n>>k;
    double a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
    sort(a,a+n);
    for(i=k;i<n-k;i++){
        y+=a[i];
        z++;
    }
    
    x=y/z;
    cout<<fixed<<setprecision(6)<<x<<endl;
}
	return 0;
}

