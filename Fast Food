#include <iostream>
using namespace std;

int main() {
	long long int t,i,j,n,x,y,z,d,e;
	cin>>t;
	while(t--){
	    x=y=z=d=e=0;
	    cin>>n;
	    long long int a[n],b[n],c=0,f=0;
	    for(i=0;i<n;i++){
	        cin>>a[i];
	        x+=a[i];
	    }
	    for(i=0;i<n;i++){
	        cin>>b[i];
	        y+=b[i];
	    }
	  
	    for(i=0;i<n;i++){
	         if(d<y){
	             d=y;
	         }
	        
	             y-=b[i];
	             y+=a[i];
	            
	         }
	       if(d<y){
	             d=y;
	         }
	    
	   
	    cout<<d<<endl;
	}
	return 0;
}
