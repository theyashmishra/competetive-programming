#include <bits/stdc++.h>
using namespace std;

int main() {
long long int t,i,n,x,y;
cin>>t;
while(t--){
    set<int>s;
    cin>>n;
    long int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
      for(i=0;i<n;i++){
        s.insert(a[i]);
    }
    x=s.size();
    cout<<x<<endl;
}
return 0;
}
