#include <iostream>
using namespace std;

int main() {
	long long int t,n,i,j,k,a;
cin>>t;
while(t--){
    cin>>k;
    for(i=0;i<64;i++){
        if(i==0){
            cout<<"O";
            k--;
        }
        else if(k!=0){
            cout<<".";
            k--;
        }
        else{
            cout<<"X";
        }
        if((i+1)%8==0){
            cout<<"\n";
        }
    }
    cout<<"\n";
}
	return 0;
}
