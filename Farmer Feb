#include<bits/stdc++.h>
using namespace std;

int isPrime(int n){
    int count  = 0;
    if(n % 2 == 0)
        count = 1;
    else {
        for(int i = 3; i < n; i++){
            if(n % i == 0)
                count++;
        }
        
    }
    if(count > 0)
        return 0;
    else
        return 1;
}

int main() {
    int n;
    cin>>n;
    while(n--){
        int a, b;
        cin>>a>>b;
        int sum = 0;
        for(int i = 1; i < INT_MAX; i++){
            sum = (a + b + i);
            
            if(isPrime(sum) ==  1){
              
                cout<<i<<"\n";
                break;
            }
        }
    }
	
	return 0;
}
