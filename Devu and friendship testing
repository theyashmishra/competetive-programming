#include <bits/stdc++.h>
using namespace std;

int main() {
int t,n,i,j,x;
cin>>t;
while(t--){
    cin>>n;
    int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
    sort(a,a+n);
    x=1;
    for(i=0;i<n-1;i++){
        if(a[i]<a[i+1]){
            x++;
        }
}
cout<<x<<endl;
}
	return 0;
}
