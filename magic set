#include <iostream>
using namespace std;

int main() {
	int t,n,i,j,m;
cin>>t;
while(t--){j=0;
    cin>>n>>m;
    int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
    for(i=0;i<n;i++){
        if(a[i]%m==0){
            j++;
        }
    }
    if(j>0){
        cout<<((1<<j)-1)<<"\n";
    }
    else{
        cout<<0<<"\n";
    }
}
	return 0;
}
