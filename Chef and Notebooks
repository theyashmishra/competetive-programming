#include <iostream>
using namespace std;

int main() {
	long long int t,x,y,z,n,i,j,k;
	cin>>t;
	while(t--){j=0;
	    cin>>x>>y>>k>>n;
	    int p[n],c[n];
	    x=x-y;
	    for(i=0;i<n;i++){
	        cin>>p[i]>>c[i];
	        if(x<=p[i]&&k>=c[i]){
	            j=1;
	        }
	    }
	    (j==1)?cout<<"LuckyChef\n":cout<<"UnluckyChef\n";
	}
	return 0;
}
