#include<stdio.h>
#include<math.h>

int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int n;
        scanf("%d",&n);
        int s,j,i,cnt=0;
        for(i=0;i<n;i++)
        {
            scanf("%d %d",&s,&j);
            if((j-s)>5)
                cnt++;
        }
        printf("%d\n",cnt);
    }
}
