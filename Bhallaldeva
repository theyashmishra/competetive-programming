#include <bits/stdc++.h>
using namespace std;

int main() {
	long long int t,n,q,k,i,j,x;
x=j=0;
	    cin>>n;
	    long long int a[n];
	    for(i=0;i<n;i++){
	        cin>>a[i];
	        x+=a[i];
	    }
	    sort(a,a+n);
	    cin>>q;
	    while(q--){j=x;
	        cin>>k;
	        for(i=k-1;i>=0;i--){
	            j=j-a[n-i-1];
	        }
	         cout<<j<<endl;
	    }
	   
	
	return 0;
}
