#include <iostream>
using namespace std;

int main() {
long long int t,n,i,j,x,k;
cin>>t;
while(t--){
    x=0;
    cin>>n>>k;
    long long int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
        x+=a[i];
    }
    x+=1;
    if(k==1){
        (x%2==0)?cout<<"even\n":cout<<"odd\n";
    }
    else{
        cout<<"even\n";
    }
}
	return 0;
}
