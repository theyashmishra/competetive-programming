#include <bits/stdc++.h>
using namespace std;

int main() {
long long int t,n,i,j,x,y,z;
cin>>t;
while(t--){x=0;z=INT_MAX;
    cin>>n;
    long long int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
        x+=a[i];
    }
    for(i=0;i<n;i++){
       if(a[i]<z){
           j=i;
           z=a[i];
       }
}
cout<<j+1<<"\n";
}
	return 0;
}
