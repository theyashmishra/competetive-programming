#include<iostream>
#include<math.h>
int main(){
    long long int t,a,b,c,i,d,e,x,y;
   std:: cin>>t;
    while(t--){
 std::cin>>x;
 a=0;
 b=0;
 long long int z[10];
 while(x>0){
     c=x%10;
     if(c!=0){
         d=c*pow(10,a);
         z[b]=d;
         b++;
     }
     a++;
     x/=10;
 }
   std::cout<<b<<"\n";
   for(i=0;i<b;i++){
       std::cout<<z[i]<<" ";
   }
   std::cout<<"\n";
    }
    return 0;
}
