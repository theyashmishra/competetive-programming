#include<bits/stdc++.h>
#include<math.h>
using namespace std;

int main(){
    long long int t,a,b,c,i;
  cin>>t;
    while(t--){
         char s1[100];
    string s;
    cin>>s1;
    for(i=0;i<strlen(s1);i++){
        if(i==0||i==strlen(s1)-1){
            s+=s1[i];
        }
        else if(i%2==0){
            s+=s1[i];
        }
    }
    cout<<s<<endl;

}
    return 0;
}
