#include <iostream>
using namespace std;

int main() {
char s1[10],s2[5];
int i,j,k,t;
cin>>t;
while(t--){
    cin>>s1>>s2;
    for(i=0;i<=5;i++){k=0;
        for(j=0;j<5;j++){
            if(s1[i+j]!=s2[j]){
                k=1;
                break;
            }
        }
        if(k==0){
            break;
        }
    }
    (k==0)?cout<<"1"<<endl:cout<<"0"<<endl;
}
	return 0;
}
