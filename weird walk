#include <iostream>
using namespace std;

int main() {
long long int t,i,j,n,x,y,z;
cin>>t;
while(t--){z=x=y=0;
    cin>>n;
    long long int a[n],b[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
    for(i=0;i<n;i++){
        cin>>b[i];
    }
    for(i=0;i<n;i++){
        x+=a[i];
        y+=b[i];
        if(x==y&&a[i]==b[i]){
            z+=a[i];
        }
    }
    cout<<z<<endl;
}
	return 0;
}
