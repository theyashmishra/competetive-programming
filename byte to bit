#include <bits/stdc++.h>
using namespace std;

int main() {
	long long int t,n,a,b,c,x;
	cin>>t;
	while(t--){x=0;long long int y=pow(2,x);
	    cin>>n;
	    if(n>26){
	    x=n/26;
	    n=n%26;
	    y=pow(2,x);}
	    if((n>10)||(x!=0&&n==0)){
	       c=y;
	       a=0;
	       b=0;
	    }
	  else if(n>2&&n<=10){
	        b=y;
	        a=0;
	        c=0;
	    }
	    else if(n<=2){
	      a=y;  b=0;c=0;
	    }
	    cout<<a<<" "<<b<<" "<<c<<" "<<"\n";
	}
	return 0;
}
