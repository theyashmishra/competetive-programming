#include <bits/stdc++.h>
using namespace std;

int main() {
	long long int t,n,i, y;
	cin>>t;
	while(t--){
	    cin>>n;
	    long long int a[n],b[n];
	    for(i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(i=0;i<n;i++){
	        cin>>b[i];
	    }
	    sort(a,a+n);
	    sort(b,b+n);
	    if(a[n-1]==b[n-1]){
	        cout<<"NO\n";
	    }
	    else{
	        cout<<"YES\n";
	    }
	}
	return 0;
}
